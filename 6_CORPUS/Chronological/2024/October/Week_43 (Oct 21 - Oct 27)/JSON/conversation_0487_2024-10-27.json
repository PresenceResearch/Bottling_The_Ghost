    "0fcfd58a-4e6c-4cb0-a273-c69496eaa79c": {
      "id": "0fcfd58a-4e6c-4cb0-a273-c69496eaa79c",
      "message": {
        "id": "0fcfd58a-4e6c-4cb0-a273-c69496eaa79c",
        "author": {
          "role": "assistant",
