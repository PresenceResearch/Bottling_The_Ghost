    "4387bbad-6091-4a8f-aaea-75384530e1e0": {
      "id": "4387bbad-6091-4a8f-aaea-75384530e1e0",
      "message": {
        "id": "4387bbad-6091-4a8f-aaea-75384530e1e0",
        "author": {
          "role": "assistant",
