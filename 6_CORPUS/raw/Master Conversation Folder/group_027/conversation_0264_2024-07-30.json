    "c4bf3768-173b-40eb-8a76-0901ccd87514": {
      "id": "c4bf3768-173b-40eb-8a76-0901ccd87514",
      "message": {
        "id": "c4bf3768-173b-40eb-8a76-0901ccd87514",
        "author": {
          "role": "assistant",
